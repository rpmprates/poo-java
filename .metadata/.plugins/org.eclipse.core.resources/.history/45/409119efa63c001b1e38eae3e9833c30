
public class TesteIR {
	public static void main(String[] args) {
		double salario = 3300.0;

		if (salario >= 1900 && salario <= 2800) {
			salario = salario - 142;
			System.out.println(salario);
		}else if (salario >= 2800.01 && salario <= 3751.00) {
			salario = salario - 350;
			System.out.println(salario);
		}else
			salario = salario - 636;
		System.out.println(salario);
	}

}
